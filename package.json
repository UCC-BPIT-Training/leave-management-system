    "lint:fix": "turbo run lint:fix",
    "fix": "npm run prettier:fix && turbo run fix"
  },
  "workspaces": [
    "apps/*"
  ],
  "dependencies": {
    "turbo": "^2.5.0"
  },
  "devDependencies": {
    "prettier": "^3.5.3"
  }
